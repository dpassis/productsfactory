	<queryString>
		<![CDATA[SELECT tb_aluno.matricula,tb_aluno.nome_aluno,tb_aluno.fone,tb_aluno.celular,tb_aluno.email, tb_parcela.numero_parcela,
tb_parcela.valor_parcela, tb_parcela.data_vencimento, 
tb_parcela.status_parcela, tb_cursos.descricao_curso 
FROM tb_aluno_curso
INNER JOIN tb_aluno ON (tb_aluno.matricula = tb_aluno_curso.pk_aluno_curso) 
INNER JOIN (tb_aula INNER JOIN tb_cursos ON (tb_cursos.codigo_curso = tb_aula.fk_curso_aula)) 
ON (tb_aluno_curso.pk_aluno_aula = tb_aula.codigo_aula) 
INNER JOIN (tb_receita INNER JOIN tb_parcela ON(tb_receita.codigo_receita = tb_parcela.fk_receita)) 
ON (tb_aluno_curso.fk_receita_aluno = tb_receita.codigo_receita) WHERE tb_parcela.status_parcela = 'Em Débito' AND tb_parcela.data_vencimento
BETWEEN $P{dataInicio} AND $P{dataFim}
ORDER BY tb_parcela.numero_parcela;]]>
	</queryString>
