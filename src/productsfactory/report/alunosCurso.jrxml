	<queryString>
		<![CDATA[SELECT tb_aluno.nome_aluno, tb_cursos.descricao_curso,tb_sala.descricao_sala,tb_funcionario.nome_funcionario,
tb_aula.dia_semana,tb_aula.horario_inicio,tb_aula.horario_fim
FROM tb_aluno_curso 
INNER JOIN tb_aluno ON (tb_aluno.matricula = tb_aluno_curso.pk_aluno_curso) 
INNER JOIN (tb_aula INNER JOIN tb_cursos ON (tb_cursos.codigo_curso = tb_aula.fk_curso_aula))
INNER JOIN tb_sala ON (tb_sala.codigo_sala = tb_aula.fk_sala)
INNER JOIN tb_funcionario ON(tb_funcionario.codigo_funcionario = tb_aula.fk_professor)
ON (tb_aluno_curso.pk_aluno_aula = tb_aula.codigo_aula)  
WHERE tb_aluno.status_aluno = 'Ativo' AND tb_aluno_curso.situacao = 'Cursando'
ORDER BY 	tb_cursos.descricao_curso, tb_aluno.nome_aluno]]>
	</queryString>
