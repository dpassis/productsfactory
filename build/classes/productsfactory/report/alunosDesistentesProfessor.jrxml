	<queryString>
		<![CDATA[SELECT tb_aluno.nome_aluno,tb_aluno.fone, tb_aluno.celular,tb_aluno.email, tb_cursos.descricao_curso,tb_funcionario.nome_funcionario
FROM tb_aluno_curso 
INNER JOIN tb_aluno ON (tb_aluno.matricula = tb_aluno_curso.pk_aluno_curso) 
INNER JOIN tb_receita ON (tb_receita.codigo_receita = tb_aluno_curso.fk_receita_aluno)
INNER JOIN (tb_aula INNER JOIN tb_cursos ON (tb_cursos.codigo_curso = tb_aula.fk_curso_aula))
INNER JOIN tb_funcionario ON(tb_funcionario.codigo_funcionario = tb_aula.fk_professor)
ON (tb_aluno_curso.pk_aluno_aula = tb_aula.codigo_aula)  
WHERE tb_aluno_curso.situacao = 'Cancelado' AND tb_aluno_curso.data_termino BETWEEN $P{dataInicio} AND $P{dataFim}
ORDER BY 	tb_aluno.nome_aluno;]]>
	</queryString>
