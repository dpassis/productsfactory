	<queryString>
		<![CDATA[SELECT
     tb_funcionario.nome_funcionario,
     tb_pagto_funcionario.descricao_pagto,
     tb_pagto_funcionario.data_transacao,
     tb_pagto_funcionario.valor_total,
     tb_cargo.descricao_cargo
FROM
     tb_funcionario  INNER JOIN tb_pagto_funcionario  ON ( tb_pagto_funcionario.fk_funcionario_pagto = tb_funcionario.codigo_funcionario)
     INNER JOIN  tb_cargo ON (tb_funcionario.fk_cargo = tb_cargo.codigo_cargo)
WHERE tb_pagto_funcionario.data_transacao BETWEEN $P{dataInicio} AND $P{dataFim}]]>
	</queryString>
